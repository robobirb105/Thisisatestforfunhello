// Building a cube example

// Class
public class Cube{
    // variable
    private int length;
    private int height;
    private int width;

    // constructors
    public cube(int.sides){
        this.height = sides;
        this.length = sides;
        this.width = sides;
    }

    // Methods
    public void getArea(){
        System.out.println((2*length * width) + (2*length * height) + (2*lewidth * height));
    }

    public void getVolume(){
        System.out.println(length * width * height);
    }

}